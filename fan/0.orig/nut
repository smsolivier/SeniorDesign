FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{   
    defaultFaces
    {
        type empty; 
    }
    inlet
    {
        type fixedValue; 
        value uniform 0; 
    }

    outlet
    {
        type zeroGradient; 
    }

    sg
    {
        type            nutUWallFunction;
        value           uniform 0;
    }
    shroud
    {
        type            nutUWallFunction;
        value           uniform 0;
    }
    leftWedge
    {
        type wedge; 
    }

    rightWedge
    {
        type wedge; 
    }
}


